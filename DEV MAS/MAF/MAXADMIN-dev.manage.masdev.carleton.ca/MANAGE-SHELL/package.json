{
  "name": "@maximo/manage-shell",
  "author": "Maximo Dev <maximodev@us.ibm.com>",
  "version": "9.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@maximo/maximo-js-api": "2.13.576",
    "@maximo/react-components": "2.13.576",
    "mobx": "5.15.7",
    "mobx-react": "5.4.4",
    "react": "17.0.2",
    "react-app-polyfill": "1.0.6",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "cd build/app && react-scripts start",
    "build": "npx --no-install maxdev-cli build --in ./src/app.xml --out ./build/app/src/ --update-ids --copy",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./src/",
    "build:production": "yarn build && yarn labels && cd build/app && react-scripts build",
    "ci:check": "npm run test:coverage && npm run lint && npm run build",
    "man": "npx --no-install maxdev-cli man",
    "test": "react-scripts test --watchAll --coverage",
    "test:coverage": "cross-env CI=true npx react-scripts test --coverage --silent --all --passWithNoTests",
    "clean": "npx --no-install maxdev-cli clean",
    "labels": "npx --no-install maxdev-cli localize -i ./src/app.xml -o ./build/app/src/labels.xml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@maximo/app-processor": "2.13.576",
    "@maximo/maxdev-cli": "2.13.576",
    "cross-env": "^7.0.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "17.0.2",
    "sinon": "19.0.2",
    "cypress": "^14.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "./src/**/*.js": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "graphite": {
    "jsonLabels": true
  },
  "resolutions": {
    "nth-check": "2.1.1",
    "parse-path": "5.0.0",
    "parse-url": "8.1.0",
    "postcss": "8.4.38",
    "semver": "7.6.0",
    "tough-cookie": "4.1.3",
    "@carbon/telemetry": "0.1.0",
    "@babel/traverse": "7.23.2",
    "path-to-regexp": "0.1.11"
  }
}