            <method name="open"><![CDATA[
 dataSet_workorder = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
 dataSet_workorder.open();
 var sqlText = new String();
 sqlText = 
   "select  " 
 + " workorder.wonum,workorder.description,workorder.cu_budget,workorder.worktype,workorder.cu_pmmgr,workorder.parent,workorder.glaccount,workorder.statusdate "
 + "from workorder " 
 + "where " +  params["where"] +  " and ( "  + "( workorder.status like '%CLOSE%'  and workorder.icbnum is null and workorder.parent is null and workorder.worktype like '%PR%'  )" + " ) " 
 + "  "
 ;

 dataSet_workorder.setQuery(sqlText);
]]></method>
            <method name="fetch"><![CDATA[if (!dataSet_workorder.fetch()) 
   return (false);
 
//         Add a line for each output column
//         The specific get method should match the data type of the output column.
 row["wonum"] = dataSet_workorder.getString("wonum"); 
 row["description"] = dataSet_workorder.getString("description"); 
 if (dataSet_workorder.getString("cu_budget") != null) 
 { 
     row["cu_budget"] = dataSet_workorder.getDouble("cu_budget"); 
 }
 row["worktype"] = dataSet_workorder.getString("worktype"); 
 row["cu_pmmgr"] = dataSet_workorder.getString("cu_pmmgr"); 
 row["parent"] = dataSet_workorder.getString("parent"); 
 row["glaccount"] = dataSet_workorder.getString("glaccount"); 
 row["statusdate"] = dataSet_workorder.getTimestamp("statusdate"); 
 return (true);]]></method>
        </script-data-set>
