            <method name="open"><![CDATA[
 dataSet_inventory = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
 dataSet_inventory.open();
 var sqlText = new String();
 sqlText = 
   "select  " 
 + " inventory.category,inventory.itemnum,i4.description,inventory.binnum,inventory.maxlevel,inventory.minlevel,inventory.issueunit,inventory.orderunit,i4.itemid "
 + "from inventory " 
 + "left outer join item i4 on   i4.itemnum  =  inventory.itemnum and i4.itemsetid  =  inventory.itemsetid "
 + "where " +  params["where"] +  " and ( "  + "( ( inventory.status  !=  'OBSOLETE'  and inventory.siteid  =  'CU'  and inventory.itemnum like 'CY%'  ) ) and ( exists (select 1 from item where itemnum = inventory.itemnum and itemsetid = inventory.itemsetid and itemtype in (select value from synonymdomain where domainid='ITEMTYPE' and maxvalue = 'ITEM' ) )  )" + " ) " 
 + "  "
 ;

 dataSet_inventory.setQuery(sqlText);
 dataSet_inventory.registerDataTranslation("description", "itemid", "item", "DESCRIPTION");
]]></method>
            <method name="fetch"><![CDATA[if (!dataSet_inventory.fetch()) 
   return (false);
 
//         Add a line for each output column
//         The specific get method should match the data type of the output column.
 row["category"] = dataSet_inventory.getString("category"); 
 row["itemnum"] = dataSet_inventory.getString("itemnum"); 
 row["description"] = dataSet_inventory.getString("description"); 
 row["binnum"] = dataSet_inventory.getString("binnum"); 
 if (dataSet_inventory.getString("maxlevel") != null) 
 { 
     row["maxlevel"] = dataSet_inventory.getDouble("maxlevel"); 
 }
 if (dataSet_inventory.getString("minlevel") != null) 
 { 
     row["minlevel"] = dataSet_inventory.getDouble("minlevel"); 
 }
 row["issueunit"] = dataSet_inventory.getString("issueunit"); 
 row["orderunit"] = dataSet_inventory.getString("orderunit"); 
 return (true);]]></method>
