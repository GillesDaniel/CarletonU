            <method name="open"><![CDATA[
 dataSet_invoice = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
 dataSet_invoice.open();
 var sqlText = new String();
 sqlText = 
   "select  " 
 + " invoice.invoicenum,invoice.description,invoice.status,invoice.cu_pmmgr,invoice.totalcost,invoice.vendor,invoice.vendorinvoicenum,invoice.invoicedate,invoice.ponum,invoice.enterdate,invoice.paiddate "
 + "from invoice " 
 + "where " +  params["where"] +  " and ( "  + "( invoice.historyflag  =  0 and invoice.siteid  =  'CU'  and invoice.enterdate  >  convert(datetime, '2021-05-06 00:00:00', 120) )" + " ) " 
 + "  "
 ;

 dataSet_invoice.setQuery(sqlText);
]]></method>
            <method name="fetch"><![CDATA[if (!dataSet_invoice.fetch()) 
   return (false);
 
//         Add a line for each output column
//         The specific get method should match the data type of the output column.
 row["invoicenum"] = dataSet_invoice.getString("invoicenum"); 
 row["description"] = dataSet_invoice.getString("description"); 
 row["status"] = dataSet_invoice.getString("status"); 
 row["cu_pmmgr"] = dataSet_invoice.getString("cu_pmmgr"); 
 if (dataSet_invoice.getString("totalcost") != null) 
 { 
     row["totalcost"] = dataSet_invoice.getDouble("totalcost"); 
 }
 row["vendor"] = dataSet_invoice.getString("vendor"); 
 row["vendorinvoicenum"] = dataSet_invoice.getString("vendorinvoicenum"); 
 row["invoicedate"] = dataSet_invoice.getDate("invoicedate"); 
 row["ponum"] = dataSet_invoice.getString("ponum"); 
 row["enterdate"] = dataSet_invoice.getTimestamp("enterdate"); 
 row["paiddate"] = dataSet_invoice.getDate("paiddate"); 
 return (true);]]></method>
