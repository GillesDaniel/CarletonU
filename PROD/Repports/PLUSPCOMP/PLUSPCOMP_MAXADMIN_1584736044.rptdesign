            <method name="open"><![CDATA[
 dataSet_companies = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
 dataSet_companies.open();
 var addLangTable = false;
 var wc =  params["where"] +  " and ( "  + "( companies.orgid  =  'PHYSPLNT'  )" + " ) " ;
 if (wc != null &amp;&amp; wc.indexOf("l_companies") > -1)
	 addLangTable = true;
 var sqlText = new String();
 sqlText = 
   "select  " 
 + " companies.company,companies.name,companies.pwemail "
 + "from companies " ;
 if(addLangTable) 
{
 sqlText += " left outer join l_companies on ( l_companies.ownerid=companies.companiesid and l_companies.langcode='"+mxReportScriptContext.getLanguageCode()+"' )";
}
 sqlText +=" "
 + "where " +  params["where"] +  " and ( "  + "( companies.orgid  =  'PHYSPLNT'  )" + " ) " 
 + "  "
 ;

 dataSet_companies.setQuery(sqlText);
]]></method>
            <method name="fetch"><![CDATA[if (!dataSet_companies.fetch()) 
   return (false);
 
//         Add a line for each output column
//         The specific get method should match the data type of the output column.
 row["company"] = dataSet_companies.getString("company"); 
 row["name"] = dataSet_companies.getString("name"); 
 row["pwemail"] = dataSet_companies.getString("pwemail"); 
 return (true);]]></method>
