<?xml version="1.0" encoding="UTF-8"?>
<presentation id="ctrl_powerapp" resultstableid="results_showlist" version="6.0.0">
    <metadata>
        <ctrlgroup groupname="EVERYONE" optionname="COMMITNOTPRG" app="GRPASSIGN" groupseq="10">
            <sigoption optionname="" app="" description="" esigenabled="" visible="" alsogrants="" alsorevokes="" prerequisite="" langcode=""/>
            <group groupname="EVERYONE" description="All Maximo Users" passwordduration="" passwordwarning="" independent="N" authallsites="N" authallgls="N" authallstorerooms="N" authlaborall="N" authlaborcrew="N" authlaborself="N" authlaborsuper="N" langcode="EN" sctemplateid="" authpersongroup="N" nullrepfac="Y" authallrepfacs="N" maxschedreport="" dfltapp="" adhoccreatelimit="" reportstoplimit="" sidenav="N" workcenter=""/>
            <ctrlcondition conditionnum="BMXCMTNOTPRG" conditionseq="10" reevaluate="Y">
                <condition conditionnum="BMXCMTNOTPRG" type="EXPRESSION" expression="(:commitinprg is null or :commitinprg!=1) and :snapshot=:no and :status in (select value from synonymdomain where domainid = 'SCHDSTATUS' and maxvalue ='DRAFT')" classname="" description="Condition display the buttons in Query section, If the commit is not in progress" nocaching=""/>
                <ctrlcondprop property="display" propertyvalue="false" conditionresult="N"/>
            </ctrlcondition>
        </ctrlgroup>
        <ctrlgroup groupname="EVERYONE" optionname="COMMITSTATUS" app="GRPASSIGN" groupseq="10">
            <sigoption optionname="" app="" description="" esigenabled="" visible="" alsogrants="" alsorevokes="" prerequisite="" langcode=""/>
            <group groupname="EVERYONE" description="All Maximo Users" passwordduration="" passwordwarning="" independent="N" authallsites="N" authallgls="N" authallstorerooms="N" authlaborall="N" authlaborcrew="N" authlaborself="N" authlaborsuper="N" langcode="EN" sctemplateid="" authpersongroup="N" nullrepfac="Y" authallrepfacs="N" maxschedreport="" dfltapp="" adhoccreatelimit="" reportstoplimit="" sidenav="N" workcenter=""/>
            <ctrlcondition conditionnum="BMXCMTNOTPRG" conditionseq="10" reevaluate="Y">
                <condition conditionnum="BMXCMTNOTPRG" type="EXPRESSION" expression="(:commitinprg is null or :commitinprg!=1) and :snapshot=:no and :status in (select value from synonymdomain where domainid = 'SCHDSTATUS' and maxvalue ='DRAFT')" classname="" description="Condition display the buttons in Query section, If the commit is not in progress" nocaching=""/>
                <ctrlcondprop property="display" propertyvalue="false" conditionresult="N"/>
            </ctrlcondition>
        </ctrlgroup>
        <ctrlgroup groupname="EVERYONE" optionname="SKDATTRIBUTE" app="GRPASSIGN" groupseq="10">
            <sigoption optionname="" app="" description="" esigenabled="" visible="" alsogrants="" alsorevokes="" prerequisite="" langcode=""/>
            <group groupname="EVERYONE" description="All Maximo Users" passwordduration="" passwordwarning="" independent="N" authallsites="N" authallgls="N" authallstorerooms="N" authlaborall="N" authlaborcrew="N" authlaborself="N" authlaborsuper="N" langcode="EN" sctemplateid="" authpersongroup="N" nullrepfac="Y" authallrepfacs="N" maxschedreport="" dfltapp="" adhoccreatelimit="" reportstoplimit="" sidenav="N" workcenter=""/>
            <ctrlcondition conditionnum="BMXATSKD" conditionseq="10" reevaluate="Y">
                <condition conditionnum="BMXATSKD" type="EXPRESSION" expression=":persistent=1" classname="" description="Condition to enable SKDAttributeName column, if persistent is true" nocaching=""/>
                <ctrlcondprop property="display" propertyvalue="false" conditionresult="N"/>
            </ctrlcondition>
        </ctrlgroup>
        <ctrlgroup groupname="EVERYONE" optionname="SKDCOMMIT" app="GRPASSIGN" groupseq="10">
            <sigoption optionname="" app="" description="" esigenabled="" visible="" alsogrants="" alsorevokes="" prerequisite="" langcode=""/>
            <group groupname="EVERYONE" description="All Maximo Users" passwordduration="" passwordwarning="" independent="N" authallsites="N" authallgls="N" authallstorerooms="N" authlaborall="N" authlaborcrew="N" authlaborself="N" authlaborsuper="N" langcode="EN" sctemplateid="" authpersongroup="N" nullrepfac="Y" authallrepfacs="N" maxschedreport="" dfltapp="" adhoccreatelimit="" reportstoplimit="" sidenav="N" workcenter=""/>
            <ctrlcondition conditionnum="BMXCMTPRG" conditionseq="10" reevaluate="Y">
                <condition conditionnum="BMXCMTPRG" type="EXPRESSION" expression=":commitinprg=1" classname="" description="Condition to show Commit In Progress static message on UI" nocaching=""/>
                <ctrlcondprop property="display" propertyvalue="false" conditionresult="N"/>
            </ctrlcondition>
        </ctrlgroup>
    </metadata>
    <page id="mainrec">
        <include id="pageHeader"/>
        <clientarea id="clientarea">
            <tabgroup id="maintabs" style="form">
                <tab default="true" id="results" label="List" type="list">
                    <menubar event="search" id="actiontoolbar1" sourcemethod="getAppSearchOptions"/>
                    <table id="results_showlist" inputmode="readonly" label="TABLE TITLE" mboname="#{mboname}" selectmode="multiple">
                        <tablebody displayrowsperpage="20" filterable="true" filterexpanded="true" id="results_showlist_tablebody">
                            <tablecol filterable="false" hidden="false" id="results_bookmark" mxevent="BOOKMARK" mxevent_desc="Add to Bookmarks" mxevent_icon="btn_addtobookmarks.gif" sortable="false" type="event"/>
                        </tablebody>
                    </table>
                </tab>
                <tab id="main" label="Main" type="insert">
                    <section id="maintab_section"/>
                </tab>
            </tabgroup>
        </clientarea>
        <include id="pageFooter"/>
    </page>
    <dialog beanclass="psdi.webclient.system.beans.QbeBean" id="searchmore" inputmode="query" label="More Search Fields">
        <section id="searchmore_1"/>
        <buttongroup id="searchmore_buttongroup">
            <pushbutton default="true" id="searchmore_buttongroup_1" label="Find" mxevent="dialogok"/>
            <pushbutton id="searchmore_buttongroup_2" label="Clear" mxevent="qbeclear"/>
            <pushbutton id="searchmore_buttongroup_3" label="Cancel" mxevent="qbecancel"/>
        </buttongroup>
    </dialog>
    <dialog beanclass="psdi.webclient.system.beans.ViewBookmarksBean" id="searchbook" label="My Bookmarks" mboname="#{mboname}">
        <table id="bookmarks_table" inputmode="readonly" selectmode="single">
            <tablebody displayrowsperpage="20" filterable="true" id="bookmarks_table_tablebody">
                <tablecol dataattribute="description" id="bookmarks_tablecolumn_2"/>
                <tablecol filterable="false" hidden="false" id="bookmarks_tablecolumn_3" mxevent="instantdelete" mxevent_desc="Delete" mxevent_icon="btn_delete.gif" sortable="false" type="event"/>
            </tablebody>
        </table>
        <buttongroup id="bookmarks_2">
            <pushbutton default="true" id="bookmarks_2_1" label="OK" mxevent="dialogok"/>
        </buttongroup>
    </dialog>
</presentation>