<?xml version="1.0" encoding="UTF-8"?>
<presentation apphelp="com.ibm.mbs.doc,reslevel/c_resource_levels_application.html" id="reslevel" ismobile="false" mboname="RESOURCELEVELS" resultstableid="results_showlist" version="7.5.0.0">
    <metadata>
        <ctrlgroup groupname="EVERYONE" optionname="HOURS" app="RESLEVEL" groupseq="10">
            <sigoption optionname="HOURS" app="RESLEVEL" description="Display field for hours resource level" esigenabled="N" visible="Y" alsogrants="" alsorevokes="" prerequisite="" langcode="EN"/>
            <group groupname="EVERYONE" description="All Maximo Users" passwordduration="" passwordwarning="" independent="N" authallsites="N" authallgls="N" authallstorerooms="N" authlaborall="N" authlaborcrew="N" authlaborself="N" authlaborsuper="N" langcode="EN" sctemplateid="" authpersongroup="N" nullrepfac="Y" authallrepfacs="N" maxschedreport="" dfltapp="" adhoccreatelimit="" reportstoplimit="" sidenav="N" workcenter=""/>
            <ctrlcondition conditionnum="RESAPPH" conditionseq="10" reevaluate="Y">
                <condition conditionnum="RESAPPH" type="EXPRESSION" expression=":type NOT IN(SELECT value FROM synonymdomain WHERE domainid = 'CALCAPPLYTO' AND maxvalue = 'QUANTITY')" classname="" description="True if the calculation does not apply to an hour" nocaching="1"/>
                <ctrlcondprop property="display" propertyvalue="false" conditionresult="N"/>
            </ctrlcondition>
        </ctrlgroup>
        <ctrlgroup groupname="EVERYONE" optionname="QUANTITY" app="RESLEVEL" groupseq="10">
            <sigoption optionname="QUANTITY" app="RESLEVEL" description="Display field for quantity resource level" esigenabled="N" visible="Y" alsogrants="" alsorevokes="" prerequisite="" langcode="EN"/>
            <group groupname="EVERYONE" description="All Maximo Users" passwordduration="" passwordwarning="" independent="N" authallsites="N" authallgls="N" authallstorerooms="N" authlaborall="N" authlaborcrew="N" authlaborself="N" authlaborsuper="N" langcode="EN" sctemplateid="" authpersongroup="N" nullrepfac="Y" authallrepfacs="N" maxschedreport="" dfltapp="" adhoccreatelimit="" reportstoplimit="" sidenav="N" workcenter=""/>
            <ctrlcondition conditionnum="RESAPPQ" conditionseq="10" reevaluate="Y">
                <condition conditionnum="RESAPPQ" type="EXPRESSION" expression=":type NOT IN(SELECT value FROM synonymdomain WHERE domainid = 'CALCAPPLYTO' AND maxvalue = 'HOURS')" classname="" description="True if the calculation does not apply to a quantity" nocaching="1"/>
                <ctrlcondprop property="display" propertyvalue="false" conditionresult="N"/>
            </ctrlcondition>
        </ctrlgroup>
    </metadata>
    <page id="mainrec">
        <include id="pageHeader"/>
        <clientarea id="clientarea">
            <tabgroup id="maintabs" style="form">
                <tab default="true" id="results" label="List" type="list">
                    <menubar event="search" id="actiontoolbar" sourcemethod="getAppSearchOptions"/>
                    <table id="results_showlist" inputmode="readonly" label="Resources levels" mboname="RESOURCELEVELS" selectmode="multiple">
                        <tablebody displayrowsperpage="20" filterable="true" filterexpanded="true" id="results_showlist_tablebody">
                            <tablecol dataattribute="RESOURCELEVELSNUM" filterable="false" id="results_showlist_column_select" mxevent="toggleselectrow" mxevent_desc="Select Row {0}" sortable="false" type="event"/>
                            <tablecol dataattribute="RESOURCELEVELSNUM" id="results_showlist_column_link" mxevent="selectrecord" mxevent_desc="Go To %1" type="link" usefieldsizegroup="false"/>
                            <tablecol dataattribute="description" id="results_showlist_column1"/>
                            <tablecol dataattribute="TYPE" id="results_showlist_column2" lookup="VALUELIST"/>
                            <tablecol dataattribute="source" id="results_showlist_column3"/>
                            <tablecol dataattribute="ORGID" id="results_showlist_column4"/>
                            <tablecol filterable="false" id="results_bookmark" mxevent="BOOKMARK" mxevent_desc="Add to Bookmarks" mxevent_icon="btn_addtobookmarks.gif" sortable="false" type="event"/>
                        </tablebody>
                    </table>
                </tab>
                <tab id="main" label="Resource Level" type="insert">
                    <section id="maintab_section">
                        <sectionrow id="maintab_sectionrow">
                            <sectioncol id="maintab_sectionrow_col1">
                                <multiparttextbox dataattribute="resourcelevelsnum" descdataattribute="description" id="maintab_attr1"/>
                                <textbox dataattribute="TYPE" id="maintab_attr2" lookup="VALUELIST" ondatachange="resetchildren"/>
                                <textbox dataattribute="SOURCE" id="maintab_attr3"/>
                                <textbox dataattribute="measureunitid" id="maintab_measureunitid" lookup="MEASUREUNIT"/>
                            </sectioncol>
                            <sectioncol id="maintab_sectionrow_col2">
                                <textbox dataattribute="ORGID" id="maintab_attr4"/>
                            </sectioncol>
                        </sectionrow>
                    </section>
                    <table id="maintab_levels_table" label="Thresholds" relationship="LEVELS">
                        <tablebody id="maintab_levels_tablebody">
                            <tablecol id="maintab_levels_tablecol1" mxevent="toggledetailstate" mxevent_desc="Show Details" type="event"/>
                            <tablecol dataattribute="lowerthreshold" id="maintab_levels_tablecol2" linkedcontrol="maintab_levels_lowerthreshold"/>
                            <tablecol dataattribute="upperthreshold" id="maintab_levels_tablecol3" linkedcontrol="maintab_levels_upperthreshold"/>
                            <tablecol dataattribute="quantity" id="maintab_levels_tablecol4" linkedcontrol="maintab_levels_quantity" sigoption="QUANTITY" sigoptiondatasrc="MAINRECORD"/>
                            <tablecol dataattribute="hours" id="maintab_levels_tablecol6" sigoption="HOURS" sigoptiondatasrc="MAINRECORD"/>
                            <tablecol id="maintab_levels_tablecol5" mxevent="toggledeleterow" mxevent_desc="Mark Row for Delete" mxevent_icon="btn_garbage.gif" type="event"/>
                        </tablebody>
                        <tabledetails id="maintab_levels_tabledetails">
                            <section id="maintab_levels_tabledetails_section">
                                <sectionrow id="maintab_levels_tabledetails_sectionrow">
                                    <sectioncol id="maintab_levels_tabledetails_sectioncol1">
                                        <textbox dataattribute="lowerthreshold" id="maintab_levels_lowerthreshold"/>
                                    </sectioncol>
                                    <sectioncol id="maintab_levels_tabledetails_sectioncol2">
                                        <textbox dataattribute="upperthreshold" id="maintab_levels_upperthreshold"/>
                                    </sectioncol>
                                    <sectioncol id="maintab_levels_tabledetails_sectioncol3">
                                        <textbox dataattribute="quantity" id="maintab_levels_quantity" sigoption="QUANTITY" sigoptiondatasrc="MAINRECORD"/>
                                    </sectioncol>
                                    <sectioncol id="maintab_levels_tabledetails_sectioncol4">
                                        <textbox dataattribute="hours" id="maintab_levels_hours" sigoption="HOURS" sigoptiondatasrc="MAINRECORD"/>
                                    </sectioncol>
                                </sectionrow>
                            </section>
                        </tabledetails>
                        <buttongroup id="maintab_levels_bgroup">
                            <pushbutton id="maintab_levels_bgroup_newrow" label="New row" mxevent="addrow" mxevent_desc="New row"/>
                        </buttongroup>
                    </table>
                </tab>
            </tabgroup>
        </clientarea>
        <include id="pageFooter"/>
    </page>
    <dialog beanclass="psdi.webclient.system.beans.QbeBean" id="searchmore" inputmode="query" label="More Search Fields">
        <section id="searchmore_1">
            <textbox dataattribute="RESOURCELEVELSNUM" id="searchmore_attr1"/>
            <textbox dataattribute="DESCRIPTION" id="searchmore_attr2"/>
            <textbox dataattribute="TYPE" id="searchmore_attr3"/>
            <textbox dataattribute="SOURCE" id="searchmore_attr4"/>
            <textbox dataattribute="ORGID" id="searchmore_attr5"/>
        </section>
        <buttongroup id="searchmore_buttongroup">
            <pushbutton default="true" id="searchmore_buttongroup_1" label="Find" mxevent="dialogok"/>
            <pushbutton id="qbe_restoreappdefault_button" label="Restore Application Defaults" mxevent="qbeclear"/>
            <pushbutton id="qbe_revisebutton" label="Revise" menutype="SEARCHMOREREVISE" targetid="searchmore"/>
            <pushbutton id="searchmore_buttongroup_3" label="Cancel" mxevent="qbecancel"/>
        </buttongroup>
    </dialog>
    <dialog beanclass="psdi.webclient.system.beans.ViewBookmarksBean" id="searchbook" label="My Bookmarks" mboname="RESOURCELEVELS">
        <table id="bookmarks_table" inputmode="readonly" selectmode="single">
            <tablebody displayrowsperpage="20" filterable="true" id="bookmarks_table_tablebody">
                <tablecol dataattribute="RESOURCELEVELSNUM" id="bookmarks_tablecolumn_1" mxevent="selectrecord" type="link"/>
                <tablecol dataattribute="description" id="bookmarks_tablecolumn_2"/>
                <tablecol dataattribute="type" id="bookmarks_tablecolumn_3"/>
                <tablecol dataattribute="source" id="bookmarks_tablecolumn_4"/>
                <tablecol filterable="false" id="bookmarks_tablecolumn_5" mxevent="instantdelete" mxevent_desc="Delete" mxevent_icon="btn_delete.gif" sortable="false" type="event"/>
            </tablebody>
        </table>
        <buttongroup id="bookmarks_2">
            <pushbutton default="true" id="bookmarks_2_1" label="OK" mxevent="dialogok"/>
        </buttongroup>
    </dialog>
    <configurationblock id="datastore_configurationblock"/>
</presentation>