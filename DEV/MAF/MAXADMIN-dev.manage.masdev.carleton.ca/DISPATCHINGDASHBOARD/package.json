{
  "name": "@maximo/dispatchingdashboard",
  "author": "Maximo Dev <maximodev@us.ibm.com>",
  "version": "9.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@maximo/map-component": "2.0.92",
    "@maximo/maximo-js-api": "2.13.576",
    "@maximo/react-components": "2.13.576",
    "@maximo/treegrid": "^0.0.116",
    "mobx": "5.15.7",
    "mobx-react": "5.4.4",
    "moment-timezone": "^0.5.40",
    "react": "17.0.2",
    "react-app-polyfill": "1.0.6",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "cd build/app && react-scripts start",
    "build": "npx --no-install maxdev-cli build --in ./src/app.xml --out ./build/app/src/ --update-ids --copy",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./src/",
    "build:production": "cd build/app && react-scripts build",
    "ci:check": "npm run test:coverage && npm run lint && npm run build",
    "man": "npx --no-install maxdev-cli man",
    "test": "react-scripts test --watchAll --coverage",
    "test:coverage": "cross-env CI=true npx react-scripts test --coverage --silent --all",
    "clean": "npx --no-install maxdev-cli clean",
    "labels": "npx --no-install maxdev-cli localize -i ./src/app.xml -o ./src/i18n/labels.json --format json",
    "proxy:local": "npx --no-install maxdev-cli setup-proxy -u wilson -p wilson --domain localhost --port 7001 --protocol http",
    "proxy:manage": "cross-env-shell npx --no-install maxdev-cli setup-proxy -k $MANAGE_API_KEY --domain main.manage.ivt811x-01.ibmmasivt.com --port 443 --protocol https"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@maximo/app-processor": "2.13.576",
    "@maximo/maxdev-cli": "2.13.576",
    "cross-env": "^7.0.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "17.0.2",
    "sinon": "7.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.js",
      "!./src/datasources/*.js",
      "!./src/model/*.js",
      "!./src/tests/sampleData/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(ol|ol-mapbox-style|geotiff|quick-lru))"
    ]
  },
  "graphite": {},
  "resolutions": {
    "nth-check": "2.1.1",
    "parse-path": "5.0.0",
    "parse-url": "8.1.0",
    "postcss": "8.4.38",
    "semver": "7.6.0",
    "tough-cookie": "4.1.3",
    "@carbon/telemetry": "0.1.0",
    "@babel/traverse": "7.23.2",
    "path-to-regexp": "0.1.11"
  }
}