{
  "name": "@maximo/techmobile",
  "author": "Maximo Dev <maximodev@us.ibm.com>",
  "version": "9.0.0",
  "private": true,
  "homepage": ".",
  "windowsInstall": true,
  "winShell": false,
  "dependencies": {
    "@maximo/map-component": "2.0.91",
    "@maximo/maximo-js-api": "2.13.576",
    "@maximo/maximo-library": "2.13.576",
    "@maximo/react-components": "2.13.576",
    "mobx": "5.15.7",
    "mobx-react": "5.4.4",
    "os-browserify": "^0.3.0",
    "react": "17.0.2",
    "react-app-polyfill": "1.0.6",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "cd build/app && react-scripts start",
    "build": "npx --no-install maxdev-cli build --in ./src/app.xml --out ./build/app/src/ --update-ids --copy",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./src/",
    "build:production": "yarn build && yarn labels && cd build/app && react-scripts build",
    "build:zip": "npx --no-install maxdev-cli upload-app -u dummy -p dummy -a techmobile --dry-run",
    "ci:check": "npm run test:coverage && npm run lint && npm run build",
    "man": "npx --no-install maxdev-cli man",
    "test": "react-scripts test --watchAll --coverage",
    "test:coverage": "cross-env CI=true npx react-scripts test --coverage --silent --all",
    "clean": "npx --no-install maxdev-cli clean",
    "labels": "npx --no-install maxdev-cli localize -i ./src/app.xml -o ./src/i18n/labels.json --format json",
    "build:maximoMobile:all:dev": "yarn build:production && npx maxdev-cli upload-app --mobile-only",
    "deploy": "npx cross-env NODE_OPTIONS=\"--max_old_space_size=30000\" GRAPHITE_SOURCEMAP=inline-source-map yarn build:production && npx maxdev-cli upload-app -u maxadmin -p maxadmin --port 7001",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "no-var": "error",
      "no-debugger": "error",
      "no-console": "error",
      "no-unused-vars": "error",
      "no-use-before-define": "error",
      "eqeqeq": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@maximo/app-processor": "2.13.576",
    "@maximo/maxdev-cli": "2.13.576",
    "@storybook/addon-essentials": "^7.6.13",
    "@storybook/addon-interactions": "^7.6.13",
    "@storybook/addon-links": "^7.6.13",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.13",
    "@storybook/preset-create-react-app": "^7.6.13",
    "@storybook/react": "^7.6.13",
    "@storybook/react-webpack5": "^7.6.13",
    "@storybook/test": "^7.6.13",
    "cross-env": "^7.0.2",
    "eslint-plugin-storybook": "^0.6.15",
    "prop-types": "^15.8.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "17.0.2",
    "sinon": "7.5.0",
    "storybook": "^7.6.17",
    "webpack": "^5.90.1"
  },
  "jest": {
    "coverageThreshold": {
      "./src/**/*.js": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(ol|ol-mapbox-style|geotiff|quick-lru))"
    ]
  },
  "graphite": {
    "platform": "maximoMobile",
    "sharedResources": {
      "maximo": {
        "library": "@maximo/maximo-library"
      }
    }
  },
  "resolutions": {
    "nth-check": "2.1.1",
    "parse-path": "5.0.0",
    "parse-url": "8.1.0",
    "postcss": "8.4.38",
    "semver": "7.6.0",
    "tough-cookie": "4.1.3",
    "@carbon/telemetry": "0.1.0",
    "@babel/traverse": "7.23.2",
    "path-to-regexp": "0.1.11"
  }
}